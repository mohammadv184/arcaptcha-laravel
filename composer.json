{
    "name": "mohammadv184/arcaptcha-laravel",
    "description": "Laravel Package for the ArCaptcha",
    "homepage": "https://github.com/mohammadv184/arcaptcha-laravel",
    "keywords": [
        "laravel",
        "arcaptcha",
        "captcha",
        "php captcha",
        "laravel captcha",
        "laravel package",
        "persian-captcha"

    ],
    "authors": [
        {
            "name": "Mohammad Abbasi",
            "email": "mohammad.v184@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "mohammadv184/arcaptcha": "^1.0",
        "laravel/framework": "^5.0|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6",
        "orchestra/testbench": "^6.19"
    },
    "autoload": {
        "psr-4": {
            "Mohammadv184\\ArCaptcha\\Laravel\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mohammadv184\\ArCaptcha\\Laravel\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "suggest": {
        "mohammadv184/arcaptcha" : "PHP library for ArCaptcha"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mohammadv184\\ArCaptcha\\Laravel\\ArCaptchaServiceProvider"
            ],
            "aliases": {
                "ArCaptcha": "Mohammadv184\\ArCaptcha\\Laravel\\Facade\\ArCaptcha"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
